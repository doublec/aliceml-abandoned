import signature SPACE        from "SPACE-sig"
import structure UnsafeGecode from "UnsafeGecode"

structure Space :> SPACE =
struct
    type space
    type description

    datatype status = BRANCH of description | FAILED | SOLVED

    open UnsafeGecode
    val new = makespace
end
