    fun testEqual' () = 
	Gen.try 1000 (Gen.pair (Gen.arraySlice Gen.int,
				Gen.arraySlice Gen.int))
		(Test.test (fn (a1, a2) => a1 = a2 = false andalso
					   a1 = a1 = true  andalso
					   a2 = a2 = true))
	
    fun testEqual () = 
	let
	    fun equal' f (a1, a2, i) = 
		if i = ArraySlice.length a1 then true
		else if f (ArraySlice.sub (a1, i), 
			   ArraySlice.sub (a2, i)) 
		     then equal' f (a1, a2, i + 1) else false

	    fun equality f (a1, a2) = (ArraySlice.length a1 = 
				       ArraySlice.length a2 andalso 
				       equal' f (a1, a2, 0))

	    fun t x = equality Int.equal x =
		      ArraySlice.equal Int.equal x
	in 
	    Gen.try 1000 (Gen.pair (Gen.arraySlice Gen.int,
				    Gen.arraySlice Gen.int)) (Test.test t)
	end

    fun testIsSorted () = 
	let
	    fun t' (f, sl, i) = if (i + 1) >= ArraySlice.length sl
			       then true
			       else (case f (ArraySlice.sub (sl, i),
					     ArraySlice.sub (sl, i + 1)) of
					 EQUAL   => t' (f, sl, i + 1)
				       | LESS    => t' (f, sl, i + 1)
				       | GREATER => false)

	    val f = Int.compare
	    fun t sl = ArraySlice.isSorted f sl = t' (f, sl, 0)
	in
	    Gen.try 1000 (Gen.arraySlice Gen.int) (Test.test t)
	end

    fun testSort () = 
	let
	    fun t sl = (ArraySlice.sort Int.compare sl;
			ArraySlice.isSorted Int.compare sl)
	in
	    Gen.try 1000 (Gen.arraySlice Gen.int) (Test.test t)
	end


